<?xml version="1.0" encoding="UTF-8" ?>

<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <defaults public="true" />
        <!-- Put your Behat services here -->

        <!-- Admin Exchange Rate Context and Pages -->
        <service id="adeliom_sylius_exchange_rate.context.ui.admin.exchange_rate" class="Tests\Adeliom\SyliusExchangeRatePlugin\Behat\Context\Ui\Admin\ExchangeRateContext">
            <argument type="service" id="adeliom_sylius_exchange_rate.page.admin.exchange_rate.index" />
        </service>

        <service id="adeliom_sylius_exchange_rate.page.admin.exchange_rate.index" class="Tests\Adeliom\SyliusExchangeRatePlugin\Behat\Page\Admin\ExchangeRate\IndexPage" parent="sylius.behat.page.admin.crud.index" public="false">
            <argument type="string">sylius_admin_exchange_rate_index</argument>
        </service>
    </services>
</container>
